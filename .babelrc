{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
        "production": {
            "plugins": [
                [
                    "transform-remove-console",
                    {
                        "exclude": [
                            "error",
                            "warn"
                        ]
                    }
                ]
            ]
        }
    }
}